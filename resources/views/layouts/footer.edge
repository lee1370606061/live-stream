        <div>
        <img src="{{homeDetails.logoSrc}}">
        </div>
        <div class="text-white d-flex flex-column justify-content-between align-items-center">
          <p class="m-0" style="font-size:1.1rem;font-weight: bold">{{homeDetails.footPromoteTop}}</p>
          <p class="m-0" style="font-size:.9rem;color:rgb(178, 178, 178)">{{homeDetails.footPromoteBottom}}</p>
        </div>
        <a class="nav-link" href="{{homeDetails.appDownloadLink}}"><button type="button" class="btn btn-primary">下载APP</button></a>
    </div>
