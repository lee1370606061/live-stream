</div>

<div class="shadow-lg px-0 bg-white">
    <footer class="mx-auto py-3 mt-3 text-center">
        @each(text in homeDetails.bottomProtocol.split('|'))
        <p>{{text}}</p>
        @endeach
    </footer>
</div>

