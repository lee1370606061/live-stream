                            var dateMonth = $('.today').text().slice(0, 6)
                            var dateDay = $('.today').text().slice(6)
                            $('#tabHeader').html(dateMonth + '<span>' + dateDay + '</span>')
                        } else {
                            $('#tabHeader').css('display', 'none')
                        }
                        if (scrollTop > todayHeight) {
                            $('#tabHeader').css({
                                'background': '#A9A9A9'
                            })
                            var dateMonth = $('.tomorrow').text().slice(0, 6)
                            var dateDay = $('.tomorrow').text().slice(6)
                            $('#tabHeader').html(dateMonth + '<span>' + dateDay + '</span>')
                        }
                        if (scrollTop > tomorrowHeight) {
                            $('#tabHeader').css({
                                'background': '#A9A9A9'
                            })
                            var dateMonth = $('.afterTomorrow').text().slice(0, 6)
                            var dateDay = $('.afterTomorrow').text().slice(6)
                            $('#tabHeader').html(dateMonth + '<span>' + dateDay + '</span>')
                        }
                    }

                }
            })
        }

        $('.match a').each(function() {
            if ($(this).attr('hot') == '1') {
                $(this).children('.list').css('font-weight', 'bold')
            }
        })
    </script>
    <script>
        function AutoScroll(obj) {
            $(obj).find("ul:first").animate({
                marginTop: "-30px"
            }, 500, function() {
                $(this).css({
                    marginTop: "0px"
                }).find("li:first").appendTo(this);
            });
        }
        $(document).ready(function() {
            setInterval('AutoScroll("#handle")', 3000);
        });
    </script>
    <!-- Initialize Swiper -->
    <script>
        var swiperH = new Swiper('.swiper-container-h', {
            pagination: '.swiper-pagination-h',
            paginationClickable: true,
            spaceBetween: 50,

            centeredSlides: true,
            autoplay: 3000,
            autoplayDisableOnInteraction: false,
            loop: true

        });
    </script>
</body>

</html>