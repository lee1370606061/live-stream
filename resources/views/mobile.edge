        // 切换状态
        function checked(check) {
            var $this = $(check);
            $(".tab ul li").removeClass("nowTab");
            $this.addClass("nowTab");
            $("#checked-" + $this.attr("key")).show().siblings(".catalog").hide();
            show($this.attr('key'))
        }


        function show(k) {
            if (k != 'zb') {
                $('#checked-qb').show();
                $('#checked-zb').hide();
                var data = $('.match').children('a');
                if (k == 'rm') {
                    data.each(function(i, v) {
                        var hot = $(this).attr('hot');
                        if (hot == 1) {
                            $(this).show();
                            $(this).attr('see', 'block')
                        } else {
                            $(this).hide();
                            $(this).attr('see', 'none')
                        }
                    })
                }
                if (k == 'zq') {
                    data.each(function() {
                        var type = $(this).attr('k')
                        if (type == 2) {
                            $(this).show();
                            $(this).attr('see', 'block')
                        } else {
                            $(this).hide();
                            $(this).attr('see', 'none')
                        }
                    })
                }
                if (k == 'lq') {
                    data.each(function() {
                        var type = $(this).attr('k')
                        if (type == 1) {
                            $(this).show();
                            $(this).attr('see', 'block')
                        } else {
                            $(this).hide();
                            $(this).attr('see', 'none')
                        }
                    })
                }
                if (k == 'qb') {
                    data.each(function() {
                        $(this).show();
                        $(this).attr('see', 'block')
                    })
                }
                var match = $('.match');
                match.each(function() {
                    var a_num = $(this).children("a[see='block']").length;
                    if (a_num == 0) {
                        $(this).children('div.noMatch').show()
                    } else {
                        $(this).children('div.noMatch').hide()
                    }
                })
            }
        }


